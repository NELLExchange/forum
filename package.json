{
  "name": "nelle-forum",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"pnpm run start:11ty\" \"pnpm run watch:css\"",
    "start:11ty": "pnpm exec eleventy --serve",
    "debug": "set DEBUG=Eleventy* && pnpm exec eleventy --serve",
    "build": "pnpm run clean && pnpm run build:css && pnpm run build:eleventy",
    "build:eleventy": "pnpm exec eleventy --pathprefix /forum/",
    "build:css": "tailwindcss -i ./src/styles/main.css -o ./_site/styles.css",
    "clean": "rimraf ./_site",
    "watch:css": "tailwindcss -i ./src/styles/main.css -o ./_site/styles.css --watch",
    "deploy:gh": "gh-pages --dist _site --no-history"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "~2.0.1",
    "@grimlink/eleventy-plugin-lucide-icons": "~1.0.4",
    "@shopify/prettier-plugin-liquid": "~1.3.2",
    "@tailwindcss/typography": "~0.5.10",
    "concurrently": "~8.2.1",
    "daisyui": "~3.9.2",
    "debug": "~4.3.4",
    "dotenv": "~16.3.1",
    "eslint": "~8.51.0",
    "eslint-config-prettier": "~9.0.0",
    "gh-pages": "~6.0.0",
    "prettier": "~3.0.3",
    "prettier-plugin-tailwindcss": "~0.5.5",
    "rimraf": "~5.0.5",
    "tailwindcss": "~3.3.3"
  }
}
